{
    "name": "offline-agency/filament-spid",
    "description": "SPID authentication plugin for Filament based on italia/spid-laravel",
    "keywords": [
        "filament",
        "spid",
        "authentication",
        "laravel",
        "offline-agency",
        "italia",
        "identity provider"
    ],
    "homepage": "https://github.com/offline-agency/filament-spid",
    "license": "MIT",
    "authors": [
        {
            "name": "Offline Agency",
            "email": "support@offlineagency.it",
            "homepage": "https://offlineagency.it",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/offline-agency/spid-laravel"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "filament/filament": "^3.0|^4.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "italia/spid-laravel": "dev-feat-add-l12",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.10|^8.0|^9.0",
        "larastan/larastan": "^2.9|^3.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.36|^3.0",
        "pestphp/pest-plugin-arch": "^2.7|^3.0",
        "pestphp/pest-plugin-laravel": "^2.4|^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "OfflineAgency\\FilamentSpid\\": "src/",
            "OfflineAgency\\FilamentSpid\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OfflineAgency\\FilamentSpid\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-filament-spid --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OfflineAgency\\FilamentSpid\\FilamentSpidServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
